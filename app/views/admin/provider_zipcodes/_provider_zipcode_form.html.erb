<%= form_tag admin_provider_zipcodes_path, method: :post, multipart: true do %>
	<fieldset class="inputs">
		<legend><span>Import Zipcodes</span></legend>
		<ol>
			<li>
				<%= label_tag :provider %>
				<%= select_tag :provider_id, options_for_select(Provider.all.map{|m| [m.name, m.id]}), prompt: "--Select Provider--" %>
			</li>
			<li>
				<%= label_tag :upload_zipcodes %>
				<%= file_field_tag :file %>
			</li>
		</ol>
	</fieldset>
	<%= submit_tag "Import" %>
<% end %>
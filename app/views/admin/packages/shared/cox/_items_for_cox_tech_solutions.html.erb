<!-- Cox Tech Solutions -->
<li style="display:none;">
	<legend><span>Set items for Cox tech solutions</span></legend>
	<ol>
		<% %w(multi_device_support single_device_support).each do |field| %>
			<li>
				<label for="provider_name" class="label">
					<%= field.humanize %>
				</label>
				<%= ff.text_field field.to_sym, style: "width: calc(78% - 22px);", onblur: "$packages.packagePriceValidation(this);" %>
			</li>
		<% end %>
	</ol>
</li>